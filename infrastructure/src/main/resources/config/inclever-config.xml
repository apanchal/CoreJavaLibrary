<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="configId"
		class="com.inclever.library.configuration.LibraryConfiguration">
		<property name="loggingMessagesBundles">
			<list>
				<value>exceptionCodes.properties</value>
				<value>DefaultHandler.properties</value>
			</list>
		</property>
		<property name="auditOn">
			<value>true</value>
		</property>
	</bean>
</beans>